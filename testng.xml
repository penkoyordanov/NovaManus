<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" parallel="false">
  <listeners>
    <listener class-name="listeners.TestListener"/>
  </listeners>
  <test name="CRUD advertisement">
    <classes>
      <class name="MakeNewAdTest"/>
       <class name="ManageAdvertiseTests"/>
      <class name="TestOnFullAd.AdDetailsTest"/>
    </classes>
  </test>
  <test name="CRD Comments on advertisement">
    <classes>
      <class name="TestOnFullAd.CRD_Comments"/>
    </classes>
  </test>
  <test name="CRUD user">
    <classes>
      <class name="RegularSignUpTest"/>
    </classes>
  </test>
  <test name="Follow">
    <classes>
      <class name="FollowTests"/>
    </classes>
  </test>
  <test name="Feed">
